{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "bracketSameLine": false,
  "singleQuote": false,
  "endOfLine": "lf",
  "importOrder": [
    "^@customTypes/(.*)$",
    "^@root/(.*)$",
    "^@src/(.*)$",
    "^@assets/(.*)$",
    "^@constants/(.*)$",
    "^@utils/(.*)$",
    "^@story/(.*)$",
    "^@tokens/(.*)$",
    "^@atoms/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
